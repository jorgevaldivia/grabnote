<!DOCTYPE html>
<html>
  <head>
    <%= render partial: "layouts/detail_admin/head" %>
  </head>
  <body ng-app="grabnote">
    <%= render partial: "layouts/detail_admin/nav" %>
      <!-- main container -->
    <div class="content">
      <div id="pad-wrapper">
        <% if notice.present? %>
          <p class="alert alert-success">
            <i class="icon-ok-sign"></i>
            <%= notice %>
          </p>
        <% end %>

        <% if alert.present? %>
          <p class="alert alert-warning">
            <i class="icon-warning-sign"></i>
            <%= notice %>
          </p>
        <% end %>

        <%= yield %>
      </div>
    </div>

  </body>
</html>
